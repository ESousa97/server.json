{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/api/server.js"
    },
    {
      "source": "/api/procedure/:id",
      "destination": "/api/server.js"
    },
    {
      "source": "/api/categories",
      "destination": "/api/server.js"
    },
    {
      "source": "/api/cards",
      "destination": "/api/server.js"
    }
  ]
}